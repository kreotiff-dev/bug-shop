DATABASE_URL=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@${POSTGRES_HOST}:${POSTGRES_PORT}/store?schema=public

PORT_SERVER=3000
PORT_CLIENT=4200

JWT_ACCESS_SECRET=secret_key
JWT_ACCESS_SECRET_EXPIRES_IN="15s"
JWT_REFRESH_SECRET=secret_key_refresh
JWT_REFRESH_SECRET_EXPIRES_IN="1m"