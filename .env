POSTGRES_USER=postgres
POSTGRES_PASSWORD=123
POSTGRES_DB=store
# POSTGRES_HOST=store-db
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# DB_HOST=localhost
# DB_PORT=5432
# DB_SCHEMA=example
